{
  "name": "knowledge-graph-rag",
  "version": "1.0.0",
  "description": "Knowledge Graph RAG Chat Assistant with Multi-Provider AI Support",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:backend": "cd backend && pnpm run dev",
    "dev:frontend": "cd frontend && pnpm run dev",
    "build": "pnpm run build:backend && pnpm run build:frontend",
    "build:backend": "cd backend && pnpm run build",
    "build:frontend": "cd frontend && pnpm run build",
    "install:all": "pnpm install && cd backend && pnpm install && cd ../frontend && pnpm install",
    "start": "concurrently \"pnpm run start:backend\" \"pnpm run start:frontend\"",
    "start:backend": "cd backend && pnpm run start",
    "start:frontend": "cd frontend && pnpm run start",
    "test": "pnpm run test:backend && pnpm run test:frontend",
    "test:backend": "cd backend && pnpm run test",
    "test:frontend": "cd frontend && pnpm run test",
    "lint": "pnpm run lint:backend && pnpm run lint:frontend",
    "lint:backend": "cd backend && pnpm run lint",
    "lint:frontend": "cd frontend && pnpm run lint",
    "clean": "pnpm run clean:backend && pnpm run clean:frontend",
    "clean:backend": "cd backend && rm -rf dist node_modules",
    "clean:frontend": "cd frontend && rm -rf dist node_modules",
    "setup": "pnpm install && pnpm run install:all",
    "health": "curl -s http://localhost:4433/health | jq .",
    "logs": "concurrently \"pnpm run logs:backend\" \"pnpm run logs:frontend\"",
    "logs:backend": "cd backend && pnpm run logs",
    "logs:frontend": "cd frontend && pnpm run logs"
  },
  "keywords": [
    "rag",
    "knowledge-graph",
    "chat",
    "ai",
    "vector-db",
    "qdrant",
    "mongodb",
    "react",
    "typescript",
    "nodejs",
    "express",
    "ollama",
    "openai",
    "anthropic"
  ],
  "author": "Abdullah Javaid",
  "email": "abdullahjavaid86@gmail.com",
  "url": "https://github.com/abdullahjavaid86",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdullahjavaid86/knowledge-graph-rag.git"
  },
  "bugs": {
    "url": "https://github.com/abdullahjavaid86/knowledge-graph-rag/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
